{
  "name": "fcc-twitch",
  "version": "0.0.1",
  "description": "A web application showing a list of user defined twitch.tv channels.",
  "main": "src/main.js",
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js,jsx"
    },
    "test": "test/*.js"
  },
  "scripts": {
    "lint": "./eslint src --ext .js --ext .jsx --cache",
    "build": "(npm run lint --silent && ./webpack -d) || beep -f 100",
    "watch": "npm-watch",
    "test": "tape test/*.js"
  },
  "author": "Dominik Rosehnal",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.20.0",
    "babel-runtime": "^6.20.0",
    "babel-cli": "^6.18.0",
    "babel-loader": "6.2.9",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "tape": "^4.5.1",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}
